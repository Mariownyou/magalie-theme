			}
		},
		{
			"name": "Markup: Link",
			"scope": [
				"meta.link",
				"markup.underline.link"
			],
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup: Link Caption",
			"scope": [
				"string.other.link.title.markdown",
				"string.other.link.description.markdown"
			],
			"settings": {
